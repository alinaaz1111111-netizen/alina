import turtle
import time
t = turtle.Turtle()
t.shape('turtle')
t.speed(0)  
t.goto(-200, 0)
Vx = 15
Vy = 40
g = -9.8
dt = 0.1
ground_level = 0
for _ in range(10):
    while Vy > 0 or t.ycor() > ground_level:
        x, y = t.position()
        x += Vx * dt
        y += Vy * dt + 0.5 * g * dt**2
        Vy += g * dt
        t.goto(x, y)
        Vy += g * dt
        t.goto(x,y)
        Vy += g * dt
        t.goto(x, y)
    Vy = -Vy * 0.7
